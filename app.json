{
	"expo": {
		"name": "DnDMe",
		"slug": "DnDMe",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/DnDMeIconNEW.png",
		"splash": {
			"image": "./assets/DnDMeIconSplashNEW.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.kirkkoster.dndme",
			"buildNumber": "0.5.4",
			"userInterfaceStyle": "automatic",
			"config": {
				"googleMobileAdsAppId": "ca-app-pub-3701578112584567~5888507574"
			},
			"infoPlist": {
				"NSUserTrackingUsageDescription": "We use Tracking to fix bugs and improve your experience."
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.kirkkoster.dndme",
			"versionCode": 5,
			"config": {
				"googleMobileAdsAppId": "ca-app-pub-3701578112584567~8644826568"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			[
				"expo-tracking-transparency",
				{
					"userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
				}
			]
		]
	}
}
